<!--pages/test_result/test_result.wxml-->
<view class="container">
  <!-- 标题 -->
  <view class='customer_lists'>
    <view class='customer_list'>
      <view class='customer_list_block'>{{username}}</view>
      <view class='customer_list_block'>{{age}}</view>
      <view class='customer_list_block'>{{sex}}</view>
      <view class='customer_list_block'>{{phone}}</view>
    </view>
  </view>
  <!-- 答题 -->
  <view class='health_topics'>
    <view class='health_topic_block'>
      <view class='health_topic_title'>1、{{oneTitle}}</view>
      <view class='health_topic'>
        <view class='health_topic_single'>
          <!-- <image class='health_topic_single_pic' src="../../images/radio-uncheck.png"></image> -->
          <image class='health_topic_single_pic' src="../../images/radio-check.png"></image>
          <view class='health_topic_single_font hover'>{{oneDA}}</view>
        </view>
      </view>
    </view>
    <view class='health_topic_block'>
      <view class='health_topic_title'>2、{{towTitle}}</view>
      <view class='health_topic'>
        <view class='health_topic_single'>
          <!-- <image class='health_topic_single_pic' src="../../images/radio-uncheck.png"></image> -->
          <image class='health_topic_single_pic' src="../../images/radio-check.png"></image>
          <view class='health_topic_single_font hover'>{{towDA}}</view>
        </view>
      </view>
    </view>
    <view class='health_topic_block'>
      <view class='health_topic_title'>3、{{thereTitle}}</view>
      <view class='health_topic'>
        <view class='health_topic_single'>
          <!-- <image class='health_topic_single_pic' src="../../images/radio-uncheck.png"></image> -->
          <image class='health_topic_single_pic' src="../../images/radio-check.png"></image>
          <view class='health_topic_single_font hover'>{{thereDA}}</view>
        </view>
      </view>
    </view>
    <view class='health_topic_block'>
      <view class='health_topic_title'>4、{{fourTitle}}</view>
      <view class='health_topic'>
        <view class='health_topic_single_01'>
          <!-- <image class='health_topic_single_pic' src="../../images/radio-uncheck.png"></image> -->
          <image class='health_topic_single_pic' src="../../images/radio-check.png"></image>
          <view class='health_topic_single_font hover'>{{fourDA}}</view>
        </view>
      </view>
    </view>
    <view class='health_topic_block'>
      <view class='health_topic_title'>5、{{fiveTitle}}</view>
      <view class='health_topic'>
        <view class='health_topic_single_01'>
          <!-- <image class='health_topic_single_pic' src="../../images/checkbox-uncheck.png"></image> -->
          <image class='health_topic_single_pic' src="../../images/checkbox-check.png"></image>
          <view class='health_topic_single_font hover'>{{fiveDA}}</view>
        </view>
      </view>
    </view>
  </view>
</view>