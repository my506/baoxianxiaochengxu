/* pages/test_see/test_see.wxss */

.customer_lists {
  width: 750rpx;
  border-top: 2rpx solid #c1c1c1;
}

.customer_list {
  width: 750rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
}

.customer_list2 {
  width: 750rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: center;
  background: #fbc7aa;
}

.customer_list_block {
  width: 24%;
  font-size: 30rpx;
  color: #333;
  text-align: center;
  margin: 20rpx 0;
}

.customer_list_block:nth-child(4) {
  width: 28%;
  font-size: 30rpx;
  color: #333;
  text-align: center;
}
